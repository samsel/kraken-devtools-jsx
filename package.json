  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samsel/kraken-devtools-jsx/issues"
  },
  "homepage": "https://github.com/samsel/kraken-devtools-jsx",
  "dependencies": {
    "react-tools": "^0.12.2"
  },
  "devDependencies": {
    "express": "^4.10.6",
    "jscs": "^1.9.0",
    "jshint": "^2.5.11",
    "kraken-devtools": "^1.0.0",
    "mocha": "^2.1.0",
    "rimraf": "^2.2.8",
    "supertest": "^0.15.0"
  }
