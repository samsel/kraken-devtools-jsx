{
  "name": "kraken-devtools-jsx",
  "version": "0.0.2",
  "description": "jsx plugin for kraken-devtools",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run jscs && npm run lint && npm run mocha",
    "jscs": "./node_modules/.bin/jscs **/*.js",
